{
  "name": "auth0",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "prettier -u -c src/**/* ext/**/*",
    "format": "prettier -u --write src/**/* ext/**/*",
    "build": "tsc && cp package*.json src/.env build/",
    "build-ext": "tsc -b tsconfig.ext.json",
    "start": "cd src && ts-node bin/www --unhandled-rejections=strict"
  },
  "dependencies": {
    "@thecurve-tv/express-utils": "git+https://github.com/thecurve-tv/express-utils.git#semver:^1.0",
    "@thecurve-tv/mongo-models": "git+https://github.com/thecurve-tv/mongo-models.git#semver:^1.0",
    "auth0": "^2.34.2",
    "bson": "^4.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "mongoose": "^5.12.7",
    "morgan": "~1.9.1",
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/auth0": "^2.33.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.41",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
